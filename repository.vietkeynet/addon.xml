<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="repository.vietkeynet"
		name="Vietkeynet Add-ons"
		version="1.0.1"
		provider-name="Vietkeynet">
  <requires>
    <import addon="xbmc.addon" version="12.0.0"/>
  </requires>
	<extension point="xbmc.addon.repository"
		name="Vietkeynet Add-on Repository">
		<info compressed="true">https://raw.githubusercontent.com/minhtuancn/kodi-vietkeynet/master/addon.xml</info>
		<checksum>https://raw.githubusercontent.com/minhtuancn/kodi-vietkeynet/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/minhtuancn/kodi-vietkeynet/master</datadir>
		<hashes>true</hashes>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Vietkeynet Entertainment</summary>
		<description lang="en"></description>
		<disclaimer lang="en"></disclaimer>
		<platform>all</platform>
	</extension>
</addon>
